#! /c/Source/iverilog-install/bin/vvp
:ivl_version "12.0 (devel)" "(s20150603-1539-g2693dd32b)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "C:\iverilog\lib\ivl\system.vpi";
:vpi_module "C:\iverilog\lib\ivl\vhdl_sys.vpi";
:vpi_module "C:\iverilog\lib\ivl\vhdl_textio.vpi";
:vpi_module "C:\iverilog\lib\ivl\v2005_math.vpi";
:vpi_module "C:\iverilog\lib\ivl\va_math.vpi";
S_000001a38d81a310 .scope module, "case_compare" "case_compare" 2 1;
 .timescale 0 0;
v000001a38d816c10_0 .var "sel", 0 0;
E_000001a38d818260 .event anyedge, v000001a38d816c10_0;
    .scope S_000001a38d81a310;
T_0 ;
    %delay 10, 0;
    %vpi_call 2 4 "$display", "\012 Driving 0" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a38d816c10_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 6 "$display", "\012 Driving 1" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001a38d816c10_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 8 "$display", "\012 Driving x" {0 0 0};
    %pushi/vec4 1, 1, 1;
    %store/vec4 v000001a38d816c10_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 10 "$display", "\012 Driving z" {0 0 0};
    %pushi/vec4 0, 1, 1;
    %store/vec4 v000001a38d816c10_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_000001a38d81a310;
T_1 ;
    %wait E_000001a38d818260;
    %load/vec4 v000001a38d816c10_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/z;
    %jmp/1 T_1.0, 4;
    %dup/vec4;
    %pushi/vec4 0, 1, 1;
    %cmp/z;
    %jmp/1 T_1.1, 4;
    %jmp T_1.2;
T_1.0 ;
    %vpi_call 2 33 "$display", "Time: %g, CASEZ: Logic 0 on sel", $time {0 0 0};
    %jmp T_1.2;
T_1.1 ;
    %vpi_call 2 36 "$display", "Time: %g, CASEZ: Logic z on sel", $time {0 0 0};
    %jmp T_1.2;
T_1.2 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
# The file index is used to find the file name in the following table.
:file_names 3;
